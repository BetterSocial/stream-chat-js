{
  "compileOnSave": false,
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictFunctionTypes": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "baseUrl": "../",
    "types": [],
    "paths": { "stream-chat": ["src/"] }
  },
  "exclude": ["node_modules"]
}
